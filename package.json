{
  "name": "poqpoq-glitch",
  "version": "0.1.0",
  "type": "module",
  "description": "Minimal embeddable 3D previewer for the BlackBox creative suite",
  "author": "Allen Partridge (p0qp0q)",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babylonjs/core": "^8.49.3",
    "@babylonjs/loaders": "^8.49.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.0.0",
    "prettier": "^3.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.1.3",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/increasinglyHuman/glitch.git"
  }
}
